# See: man pkg-config
prefix=/mingw32
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Python
Description: Build a C extension for Python
Requires:
Version: 3.12
Libs.private:  -lversion -lshlwapi -lpathcch -lbcrypt
Libs: -L${libdir} -lpython3.12
Cflags: -I${includedir}/python3.12
